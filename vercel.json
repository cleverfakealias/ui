{
  "version": 2,
  "builds": [
    { "src": "nuxt-vue/package.json", "use": "@vercel/node" },
    { "src": "nuxt-vue/api/send_email.py", "use": "@vercel/python" },
    { "src": "nuxt-vue/api/test.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/send_email/(.*)", "dest": "nuxt-vue/api/send_email.py" },
    { "src": "/api/test/(.*)", "dest": "nuxt-vue/api/test.py" },
    { "src": "/(.*)", "dest": "nuxt-vue/$1" }
  ]
}
