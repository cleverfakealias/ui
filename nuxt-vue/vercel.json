{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/node" },
    { "src": "api/send_email.py", "use": "@vercel/python" },
    { "src": "api/test.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/send_email/(.*)", "dest": "/api/send_email.py" },
    { "src": "/api/test/(.*)", "dest": "/api/test.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
